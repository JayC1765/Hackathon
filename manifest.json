{
  "manifest_version": 2,
  "name": "Our Extension",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "A plain text description",
  "content_scripts": [
    {
      "matches": [
        "*://www.facebook.com/*",
        "*://www.instagram.com/*",
        "*://www.youtube.com/*",
        "*://www.9gag.com/*",
        "*://*.reddit.com/*",
        "*://www.tiktok.com/*",
        "*://www.twitter.com/*"
      ],
      "js": [
        "./sentance.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "*.jpg",
    "*.png"
  ]
}
